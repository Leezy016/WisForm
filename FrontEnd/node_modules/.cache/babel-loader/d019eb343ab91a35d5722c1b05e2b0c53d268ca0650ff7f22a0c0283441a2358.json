{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from './NavBar.vue';\nexport default {\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      users: [],\n      // 用户列表\n      newUser: {\n        username: '',\n        userType: ''\n      } // 添加新用户表单数据\n    };\n  },\n  mounted() {\n    // 模拟从后端获取用户列表\n    this.users = [{\n      username: '用户1',\n      userType: '管理员'\n    }\n    // 添加更多用户\n    ];\n  },\n  methods: {\n    addUser() {\n      // 添加新用户\n      this.users.push({\n        username: this.newUser.username,\n        userType: this.newUser.userType\n      });\n      // 清空表单数据\n      this.newUser.username = '';\n      this.newUser.userType = '';\n    },\n    editUser(user) {\n      // 编辑用户，可以弹出模态框进行编辑\n      console.log('编辑用户:', user);\n    },\n    deleteUser(user) {\n      // 删除用户，可以弹出确认对话框\n      const index = this.users.indexOf(user);\n      if (index !== -1) {\n        this.users.splice(index, 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","components","data","users","newUser","username","userType","mounted","methods","addUser","push","editUser","user","console","log","deleteUser","index","indexOf","splice"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/UserManagement.vue"],"sourcesContent":["<template>\r\n  <div>\r\n      <NavBar/>\r\n    </div>\r\n  <div class=\"user-management\">\r\n    \r\n\r\n    <h2>用户管理</h2>\r\n    <form @submit.prevent=\"addUser\">\r\n      <input type=\"text\" v-model=\"newUser.username\" placeholder=\"用户名\">\r\n      <input type=\"text\" v-model=\"newUser.userType\" placeholder=\"用户类型\">\r\n      <button type=\"submit\">添加用户</button>\r\n    </form>\r\n    \r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>用户名</th>\r\n          <th>用户类型</th>\r\n          <th>操作</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(user, index) in users\" :key=\"index\">\r\n          <td>{{ user.username }}</td>\r\n          <td>{{ user.userType }}</td>\r\n          <td>\r\n            <button @click=\"editUser(user)\">编辑</button>\r\n            <button @click=\"deleteUser(user)\">删除</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from './NavBar.vue'; \r\nexport default {\r\n  components: {\r\n      NavBar,\r\n    },\r\n  data() {\r\n    return {\r\n      users: [], // 用户列表\r\n      newUser: { username: '', userType: '' } // 添加新用户表单数据\r\n    };\r\n  },\r\n  mounted() {\r\n    // 模拟从后端获取用户列表\r\n    this.users = [\r\n      { username: '用户1', userType: '管理员' },\r\n      // 添加更多用户\r\n    ];\r\n  },\r\n  methods: {\r\n    addUser() {\r\n      // 添加新用户\r\n      this.users.push({ username: this.newUser.username, userType: this.newUser.userType });\r\n      // 清空表单数据\r\n      this.newUser.username = '';\r\n      this.newUser.userType = '';\r\n    },\r\n    editUser(user) {\r\n      // 编辑用户，可以弹出模态框进行编辑\r\n      console.log('编辑用户:', user);\r\n    },\r\n    deleteUser(user) {\r\n      // 删除用户，可以弹出确认对话框\r\n      const index = this.users.indexOf(user);\r\n      if (index !== -1) {\r\n        this.users.splice(index, 1);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-management{\r\n    margin: auto;\r\n  }\r\n</style>\r\n"],"mappings":";AAqCA,OAAOA,MAAK,MAAO,cAAc;AACjC,eAAe;EACbC,UAAU,EAAE;IACRD;EACF,CAAC;EACHE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,OAAO,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,EAAE;IAC1C,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACJ,KAAI,GAAI,CACX;MAAEE,QAAQ,EAAE,KAAK;MAAEC,QAAQ,EAAE;IAAM;IACnC;IAAA,CACD;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR;MACA,IAAI,CAACN,KAAK,CAACO,IAAI,CAAC;QAAEL,QAAQ,EAAE,IAAI,CAACD,OAAO,CAACC,QAAQ;QAAEC,QAAQ,EAAE,IAAI,CAACF,OAAO,CAACE;MAAS,CAAC,CAAC;MACrF;MACA,IAAI,CAACF,OAAO,CAACC,QAAO,GAAI,EAAE;MAC1B,IAAI,CAACD,OAAO,CAACE,QAAO,GAAI,EAAE;IAC5B,CAAC;IACDK,QAAQA,CAACC,IAAI,EAAE;MACb;M