{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  props: ['id', 'item', 'content'],\n  // 接收从父组件传递过来的 title \n  data() {\n    return {\n      getErrorMessage: '',\n      submitErrorMessage: ''\n    };\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    submitForm() {\n      console.log(this.content);\n      axios.post('http://localhost:8080/formchange', {\n        //id:id\n        Item: this.item,\n        ItemValue: this.content\n      }).then(response => {\n        if (response.data.success) {\n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push({\n            name: 'FormViewDetail',\n            params: {\n              title: this.title\n            }\n          });\n        } else {\n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试';\n          alert(this.submiterrorMessage);\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;\n        } else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });\n    },\n    goBack() {\n      this.$router.push({\n        name: 'FormViewDetail',\n        params: {\n          title: this.title\n        }\n      });\n    }\n  },\n  created() {\n    this.getItems();\n    this.content = Array(this.item.length).fill('');\n  }\n};","map":{"version":3,"names":["NavBar","axios","props","data","getErrorMessage","submitErrorMessage","components","methods","submitForm","console","log","content","post","Item","item","ItemValue","then","response","success","alert","$router","push","name","params","title","message","submiterrorMessage","catch","error","goBack","created","getItems","Array","length","fill"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormChange.vue"],"sourcesContent":["<template>  \n    <div>\n      <NavBar/>\n    </div>\n    <div class=\"form-change\">  \n      <p>{{ title }}</p>  \n      <div v-for=\"(itemName, index) in item\" :key=\"index\" class=\"form-group\">  \n        <label>{{ itemName }}</label>  \n          <input v-model=\"content[index]\" />  \n        </div>  \n        <button type=\"button\" class=\"submit-btn\" @click=\"submitForm\">提交</button> \n        <button type=\"button\" class=\"return-btn\" @click=\"goBack\">返回</button>\n        <p v-if=\"getErrorMessage\" class=\"errorMessage\">{{ getErrorMessage }}</p> \n        <p v-if=\"submitErrorMessage\" class=\"errorMessage\">{{ submitErrorMessage }}</p> \n    </div>  \n  </template>  \n    \n  <script>  \n  import NavBar from './NavBar.vue'; \n  import axios from 'axios';\n  export default {  \n    props: ['id','item','content'], // 接收从父组件传递过来的 title \n    data() {  \n    return {  \n      getErrorMessage:'',\n      submitErrorMessage:''\n    };  \n    },  \n    components: {\n      NavBar,\n    },\n    methods:{\n    submitForm() {  \n      console.log(this.content);  \n      axios.post('http://localhost:8080/formchange', {\n        //id:id\n        Item:this.item,\n        ItemValue:this.content\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push({ name: 'FormViewDetail', params: { title: this.title} });\n        }\n        else {  \n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试'; \n          alert(this.submiterrorMessage); \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;   \n        }\n        else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });  \n    },   \n    goBack(){\n      this.$router.push({ name: 'FormViewDetail', params: { title: this.title} });\n    }, \n    }, \n    \n    created() {  \n      this.getItems(); \n      this.content = Array(this.item.length).fill(''); \n    },\n\n  };  \n  </script>\n\n  <style>\n  .form-detail{\n    margin: auto;\n  }\n</style>\n"],"mappings":";AAkBE,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAK,EAAE,CAAC,IAAI,EAAC,MAAM,EAAC,SAAS,CAAC;EAAE;EAChCC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,eAAe,EAAC,EAAE;MAClBC,kBAAkB,EAAC;IACrB,CAAC;EACD,CAAC;EACDC,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,OAAO,EAAC;IACRC,UAAUA,CAAA,EAAG;MACXC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,OAAO,CAAC;MACzBV,KAAK,CAACW,IAAI,CAAC,kCAAkC,EAAE;QAC7C;QACAC,IAAI,EAAC,IAAI,CAACC,IAAI;QACdC,SAAS,EAAC,IAAI,CAACJ;MACjB,CAAC,EACAK,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACd,IAAI,CAACe,OAAO,EAAE;UACzB;UACAC,KAAK,CAAC,SAAS,CAAC;UAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAEC,IAAI,EAAE,gBAAgB;YAAEC,MAAM,EAAE;cAAEC,KAAK,EAAE,IAAI,CAACA;YAAK;UAAE,CAAC,CAAC;QAC7E,OACK;UACH,IAAI,CAACnB,kBAAiB,GAAIY,QAAQ,CAACd,IAAI,CAACsB,OAAM,IAAK,cAAc;UACjEN,KAAK,CAAC,IAAI,CAACO,kBAAkB,CAAC;QAChC;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACX,QAAQ,EAAE;UAClB;UACA,IAAI,CAACZ,kBAAiB,GAAIuB,KAAK,CAACX,QAAQ,CAACd,IAAI;QAC/C,OACK;UACH,IAAI,CAACE,kBAAiB,GAAI,cAAc;QAC1C;MACF,CAAC,CAAC;IACJ,CAAC;IACDwB,MAAMA,CAAA,EAAE;MACN,IAAI,CAACT,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,MAAM,EAAE;UAAEC,KAAK,EAAE,IAAI,CAACA;QAAK;MAAE,CAAC,CAAC;IAC7E;EACA,CAAC;EAEDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC;IACf,IAAI,CAACpB,OAAM,GAAIqB,KAAK,CAAC,IAAI,CAAClB,IAAI,CAACmB,MAAM,CAAC,CAACC,IAAI,CAAC,EAAE,CAAC;EACjD;AAEF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}