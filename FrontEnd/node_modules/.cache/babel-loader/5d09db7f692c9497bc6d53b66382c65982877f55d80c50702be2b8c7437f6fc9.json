{"ast":null,"code":"export default {\n  data() {\n    return {\n      selectedForm: null,\n      forms: [{\n        id: 1,\n        name: '表单1',\n        fields: [{\n          id: 'field1',\n          label: '字段1',\n          type: 'text'\n        }, {\n          id: 'field2',\n          label: '字段2',\n          type: 'text'\n        }]\n      }, {\n        id: 2,\n        name: '表单2',\n        fields: [{\n          id: 'field1',\n          label: '字段1',\n          type: 'text'\n        }, {\n          id: 'field2',\n          label: '字段2',\n          type: 'text'\n        }, {\n          id: 'field3',\n          label: '字段3',\n          type: 'text'\n        }]\n      }\n      // 其他表单配置\n      ],\n      formFields: []\n    };\n  },\n  methods: {\n    loadForm() {\n      const selectedForm = this.forms.find(form => form.id === parseInt(this.selectedForm));\n      this.formFields = selectedForm ? selectedForm.fields : [];\n    },\n    submitForm() {\n      // 处理表单提交逻辑\n      console.log('提交表单:', this.formFields);\n    }\n  }\n};","map":{"version":3,"names":["data","selectedForm","forms","id","name","fields","label","type","formFields","methods","loadForm","find","form","parseInt","submitForm","console","log"],"sources":["C:\\Users\\32118\\Desktop\\无脑赶工\\my-project\\src\\components\\DynamicForm.vue"],"sourcesContent":["<!-- src/components/DynamicForm.vue -->\r\n\r\n<template>\r\n  <div class=\"dynamic-form\">\r\n    <h2>选择表单</h2>\r\n    <select v-model=\"selectedForm\" @change=\"loadForm\">\r\n      <option v-for=\"form in forms\" :value=\"form.id\">{{ form.name }}</option>\r\n    </select>\r\n    <form v-if=\"formFields.length > 0\" @submit.prevent=\"submitForm\">\r\n      <div v-for=\"field in formFields\" :key=\"field.id\">\r\n        <label :for=\"field.id\">{{ field.label }}：</label>\r\n        <input :type=\"field.type\" :id=\"field.id\" v-model=\"field.value\">\r\n      </div>\r\n      <button type=\"submit\">提交</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedForm: null,\r\n      forms: [\r\n        { id: 1, name: '表单1', fields: [{ id: 'field1', label: '字段1', type: 'text' }, { id: 'field2', label: '字段2', type: 'text' }] },\r\n        { id: 2, name: '表单2', fields: [{ id: 'field1', label: '字段1', type: 'text' }, { id: 'field2', label: '字段2', type: 'text' }, { id: 'field3', label: '字段3', type: 'text' }] }\r\n        // 其他表单配置\r\n      ],\r\n      formFields: []\r\n    };\r\n  },\r\n  methods: {\r\n    loadForm() {\r\n      const selectedForm = this.forms.find(form => form.id === parseInt(this.selectedForm));\r\n      this.formFields = selectedForm ? selectedForm.fields : [];\r\n    },\r\n    submitForm() {\r\n      // 处理表单提交逻辑\r\n      console.log('提交表单:', this.formFields);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dynamic-form {\r\n  max-width: 400px;\r\n  margin: auto;\r\n}\r\n\r\n.dynamic-form h2 {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.dynamic-form select {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.dynamic-form form div {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.dynamic-form button {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":"AAmBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,KAAK;QAAEC,MAAM,EAAE,CAAC;UAAEF,EAAE,EAAE,QAAQ;UAAEG,KAAK,EAAE,KAAK;UAAEC,IAAI,EAAE;QAAO,CAAC,EAAE;UAAEJ,EAAE,EAAE,QAAQ;UAAEG,KAAK,EAAE,KAAK;UAAEC,IAAI,EAAE;QAAO,CAAC;MAAE,CAAC,EAC5H;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,KAAK;QAAEC,MAAM,EAAE,CAAC;UAAEF,EAAE,EAAE,QAAQ;UAAEG,KAAK,EAAE,KAAK;UAAEC,IAAI,EAAE;QAAO,CAAC,EAAE;UAAEJ,EAAE,EAAE,QAAQ;UAAEG,KAAK,EAAE,KAAK;UAAEC,IAAI,EAAE;QAAO,CAAC,EAAE;UAAEJ,EAAE,EAAE,QAAQ;UAAEG,KAAK,EAAE,KAAK;UAAEC,IAAI,EAAE;QAAO,CAAC;MAAE;MACzK;MAAA,CACD;MACDC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,