{"ast":null,"code":"import NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      item: '',\n      content: [''],\n      getErrorMessage: ''\n    };\n  },\n  methods: {\n    search() {\n      axios.post('http://localhost:8080/', {\n        item: this.item\n      }).then(response => {\n        if (response.data.success) {\n          this.content = response.data.content;\n        } else {\n          this.getErrorMessage = response.data.message || '信息获取失败，请稍后再试';\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;\n        } else {\n          this.getErrorMessage = '信息失败，请稍后再试';\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavBar","axios","components","data","item","content","getErrorMessage","methods","search","post","then","response","success","message","catch","error"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/SearchByItem.vue"],"sourcesContent":["<template>\n    <div class=\"search-by-item\">\n      <div>\n        <NavBar/>\n      </div>\n      <p>请输入需要检索的表项名</p>\n      <input v-model=\"item\" />\n      <button type=\"button\" class=\"search-btn\" @click=\"search\">搜索</button> \n\n    <div>  \n    <table>  \n      <tbody>  \n        <tr v-for=\"(value, index) in content\" :key=\"index\">  \n          <td>{{ value }}</td>  \n        </tr>  \n      </tbody>  \n    </table>  \n  </div>\n</div>\n\n</template>\n\n<script>\nimport NavBar from './NavBar.vue'; \nimport axios from 'axios';\nexport default {\n  components: {\n      NavBar,\n    },\n    data() {  \n    return {  \n      item:'',\n      content:[''],\n      getErrorMessage:'',\n    };  \n    }, \n    methods:{\n        search(){\n        axios.post('http://localhost:8080/', {  \n        item:this.item\n        })  \n        .then(response => {  \n          if (response.data.success) { \n            this.content=response.data.content\n          }\n          else {  \n            this.getErrorMessage = response.data.message || '信息获取失败，请稍后再试';  \n          }\n         })  \n        .catch(error => {  \n          if (error.response) {  \n            // 后端返回的错误信息\n            this.getErrorMessage = error.response.data;   \n          }\n          else {\n            this.getErrorMessage = '信息失败，请稍后再试';\n          }\n        });\n        }\n    }\n\n}\n</script>\n<style>  \n  \n.search-by-item{  \n  max-width: 200px;\n  margin: auto;\n}  \n</style>"],"mappings":"AAuBA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,UAAU,EAAE;IACRF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAC,EAAE;MACPC,OAAO,EAAC,CAAC,EAAE,CAAC;MACZC,eAAe,EAAC;IAClB,CAAC;EACD,CAAC;EACDC,OAAO,EAAC;IACJC,MAAMA,CAAA,EAAE;MACRP,KAAK,CAACQ,IAAI,CAAC,wBAAwB,EAAE;QACrCL,IAAI,EAAC,IAAI,CAACA;MACV,CAAC,EACAM,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACR,IAAI,CAACS,OAAO,EAAE;UACzB,IAAI,CAACP,OAAO,GAACM,QAAQ,CAACR,IAAI,CAACE,OAAM;QACnC,OACK;UACH,IAAI,CAACC,eAAc,GAAIK,QAAQ,CAACR,IAAI,CAACU,OAAM,IAAK,cAAc;QAChE;MACD,CAAC,EACDC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACJ,QAAQ,EAAE;UAClB;UACA,IAAI,CAACL,eAAc,GAAIS,KAAK,CAACJ,QAAQ,CAACR,IAAI;QAC5C,OACK;UACH,IAAI,CAACG,eAAc,GAAI,YAAY;QACrC;MACF,CAAC,CAAC;IACF;EACJ;AAEJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}