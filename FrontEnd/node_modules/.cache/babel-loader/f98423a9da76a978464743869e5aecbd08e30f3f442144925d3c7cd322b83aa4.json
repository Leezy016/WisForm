{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  props: ['id', 'titile'],\n  // 接收从父组件传递过来的\n  data() {\n    return {\n      item: this.$store.state.item,\n      content: this.$store.state.content,\n      getErrorMessage: '',\n      submitErrorMessage: ''\n    };\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    submitForm() {\n      console.log(this.content);\n      axios.post('http://localhost:8080/viewform/form-change', {\n        id: this.id,\n        Item: this.item,\n        ItemValue: this.content\n      }).then(response => {\n        if (response.data.success) {\n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push({\n            name: 'FormViewDetail',\n            params: {\n              title: this.title\n            }\n          });\n        } else {\n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试';\n          alert(this.submiterrorMessage);\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;\n        } else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });\n    },\n    goBack() {\n      this.$router.push({\n        name: 'FormViewDetail',\n        params: {\n          title: this.title\n        }\n      });\n    }\n  },\n  created() {\n    console.log(\"item=\");\n    console.log(this.item);\n  }\n};","map":{"version":3,"names":["NavBar","axios","props","data","item","$store","state","content","getErrorMessage","submitErrorMessage","components","methods","submitForm","console","log","post","id","Item","ItemValue","then","response","success","alert","$router","push","name","params","title","message","submiterrorMessage","catch","error","goBack","created"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormChange.vue"],"sourcesContent":["<template>  \n    <div>\n      <NavBar/>\n    </div>\n    <div class=\"form-change\">  \n\n      <div class=\"welcome\">  \n        <img src='@/assets/welcome.png' style=\"width: 500px; height: 80px;\"  />  \n        </div>\n\n      <p>{{ title }}</p>  \n\n      <div v-for=\"(itemName, index) in item\" :key=\"index\" class=\"form-group\">  \n        <label class=\"label\">{{ itemName }}</label>  \n        <div style=\"margin: 5px 0\" />\n        <el-input\n        v-model=\"content[index]\"\n        style=\"width: 400px\"\n        autosize\n        type=\"textarea\"\n        @focus=\"keyJudge(item[index],content[index],index)\"\n        @blur=\"judge(item[index],content[index],index)\"\n        \n        />\n        <div style=\"margin: 15px 0\" />\n      </div>  \n\n      <div v-for=\"(itemName, index) in this.$store.state.item\" :key=\"index\" class=\"form-group\">  \n        <label>{{ itemName }}</label> \n        <div style=\"margin: 5px 0\" />\n        <el-input\n        v-model=\"this.$store.state.content[index]\"\n        style=\"width: 400px\"\n        autosize\n        type=\"textarea\"\n        @focus=\"keyJudge(item[index],content[index],index)\"\n        @blur=\"judge(item[index],content[index],index)\"\n        /> \n        </div>  \n        <button type=\"button\" class=\"submit-btn\" @click=\"submitForm\">提交</button> \n        <button type=\"button\" class=\"return-btn\" @click=\"goBack\">返回</button>\n        <p v-if=\"getErrorMessage\" class=\"errorMessage\">{{ getErrorMessage }}</p> \n        <p v-if=\"submitErrorMessage\" class=\"errorMessage\">{{ submitErrorMessage }}</p> \n    </div>  \n  </template>  \n    \n  <script>  \n  import NavBar from './NavBar.vue'; \n  import axios from 'axios';\n  export default {  \n    props: ['id','titile'], // 接收从父组件传递过来的\n    data() {  \n    return {  \n      item:this.$store.state.item,\n      content:this.$store.state.content,\n      getErrorMessage:'',\n      submitErrorMessage:''\n    };  \n    },  \n    components: {\n      NavBar,\n    },\n    methods:{\n    submitForm() {  \n      console.log(this.content);  \n      axios.post('http://localhost:8080/viewform/form-change', {\n        id:this.id,\n        Item:this.item,\n        ItemValue:this.content\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push({ name: 'FormViewDetail', params: { title: this.title} });\n        }\n        else {  \n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试'; \n          alert(this.submiterrorMessage); \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;   \n        }\n        else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });  \n    },   \n    goBack(){\n      this.$router.push({ name: 'FormViewDetail', params: { title: this.title} });\n    }, \n    }, \n    \n    created() {   \n      console.log(\"item=\");\n      console.log(this.item);\n    },\n\n  };  \n  </script>\n\n  <style>\n  .form-change{\n    margin: auto;\n  }\n</style>\n"],"mappings":";AA+CE,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAK,EAAE,CAAC,IAAI,EAAC,QAAQ,CAAC;EAAE;EACxBC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,IAAI;MAC3BG,OAAO,EAAC,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,OAAO;MACjCC,eAAe,EAAC,EAAE;MAClBC,kBAAkB,EAAC;IACrB,CAAC;EACD,CAAC;EACDC,UAAU,EAAE;IACVV;EACF,CAAC;EACDW,OAAO,EAAC;IACRC,UAAUA,CAAA,EAAG;MACXC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,OAAO,CAAC;MACzBN,KAAK,CAACc,IAAI,CAAC,4CAA4C,EAAE;QACvDC,EAAE,EAAC,IAAI,CAACA,EAAE;QACVC,IAAI,EAAC,IAAI,CAACb,IAAI;QACdc,SAAS,EAAC,IAAI,CAACX;MACjB,CAAC,EACAY,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACjB,IAAI,CAACkB,OAAO,EAAE;UACzB;UACAC,KAAK,CAAC,SAAS,CAAC;UAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAEC,IAAI,EAAE,gBAAgB;YAAEC,MAAM,EA