{"ast":null,"code":"import NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      item: '',\n      content: [],\n      getErrorMessage: ''\n    };\n  },\n  methods: {\n    search() {\n      axios.post('http://localhost:8080/search', {\n        title: this.item\n      }).then(response => {\n        if (response.data.success) {\n          this.content = response.data.ans;\n        } else {\n          this.getErrorMessage = response.data.message || '信息获取失败，请稍后再试';\n        }\n      }).catch(error => {\n        if (error.response) {\n          this.getErrorMessage = error.response.data;\n        } else {\n          this.getErrorMessage = '信息失败，请稍后再试';\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavBar","axios","components","data","item","content","getErrorMessage","methods","search","post","title","then","response","success","ans","message","catch","error"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/SearchByItem.vue"],"sourcesContent":["<template>\n    <div class=\"search-by-item\">\n      <div>\n        <NavBar/>\n      </div>\n      <div class=\"welcome\">  \n        <img src='@/assets/welcome.png' style=\"width: 500px; height: 80px;\"  />  \n        </div>\n      <div class=\"text\">\n        <p style=\"color: black;\">请输入需要查看的表项名</p>\n      </div>\n\n      <el-input v-model=\"item\" style=\"width: 240px\" placeholder=\"Please input\" />\n      <button type=\"button\" class=\"search-btn\" @click=\"search\">搜索</button> \n\n    <div>  \n    <table>  \n      <tbody>  \n        <tr v-for=\"(value, index) in content\" :key=\"index\">  \n          <td>{{ value }}</td>  \n        </tr>  \n      </tbody>  \n    </table>  \n  </div>\n</div>\n\n</template>\n\n<script>\nimport NavBar from './NavBar.vue'; \nimport axios from 'axios';\nexport default {\n  components: {\n      NavBar,\n    },\n    data() {  \n    return {  \n      item:'',\n      content:[],\n      getErrorMessage:'',\n    };  \n    }, \n    methods:{\n        search(){\n        axios.post('http://localhost:8080/search', {  \n        title:this.item\n        })  \n        .then(response => {  \n          if (response.data.success) { \n            this.content=response.data.ans\n          }\n          else {  \n            this.getErrorMessage = response.data.message || '信息获取失败，请稍后再试';  \n          }\n         })  \n        .catch(error => {  \n          if (error.response) {  \n            this.getErrorMessage = error.response.data;   \n          }\n          else {\n            this.getErrorMessage = '信息失败，请稍后再试';\n          }\n        });\n        }\n    }\n\n}\n</script>\n<style>  \n  .container {  \n    display: flex;  \n    flex-direction: row;  \n  }  \n    \n  .search-by-item{  \n    max-width: 400px;\n    margin: auto;\n  } \n  .table .view-btn {  \n  margin-left: 10px; /* 左边距 */  \n  padding: 5px 10px; /* 内边距 */  \n  background-color: #6292ff; /* 蓝色背景 */  \n  color: #fff; /* 白色文本 */  \n  border: none; /* 无边框 */  \n  border-radius: 8px; /* 圆角 */  \n  cursor: pointer; /* 鼠标悬停时变为小手图标 */  \n  transition: background-color 0.3s ease; /* 背景色过渡效果 */  \n  margin-top: 7px;\n}  \n  \n/* 填写按钮点击后的样式（可选） */  \n.table .view-btn:active {  \n  background-color: #0056b3; /* 深蓝色背景 */  \n} \n</style>"],"mappings":"AA6BA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,UAAU,EAAE;IACRF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAC,EAAE;MACPC,OAAO,EAAC,EAAE;MACVC,eAAe,EAAC;IAClB,CAAC;EACD,CAAC;EACDC,OAAO,EAAC;IACJC,MAAMA,CAAA,EAAE;MACRP,KAAK,CAACQ,IAAI,CAAC,8BAA8B,EAAE;QAC3CC,KAAK,EAAC,IAAI,CAACN;MACX,CAAC,EACAO,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACT,IAAI,CAACU,OAAO,EAAE;UACzB,IAAI,CAACR,OAAO,GAACO,QAAQ,CAACT,IAAI,CAACW,GAAE;QAC/B,OACK;UACH,IAAI,CAACR,eAAc,GAAIM,QAAQ,CAACT,IAAI,CAACY,OAAM,IAAK,cAAc;QAChE;MACD,CAAC,EACDC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACL,QAAQ,EAAE;UAClB,IAAI,CAACN,eAAc,GAAIW,KAAK,CAACL,QAAQ,CAA