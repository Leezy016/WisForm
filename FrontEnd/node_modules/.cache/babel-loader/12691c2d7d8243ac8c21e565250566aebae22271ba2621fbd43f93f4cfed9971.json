{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      users: [],\n      // 用户列表\n      newUser: {\n        username: '',\n        userType: ''\n      } // 添加新用户表单数据\n    };\n  },\n  mounted() {\n    // 模拟从后端获取用户列表\n    this.users = [{\n      username: '用户1',\n      userType: '管理员'\n    }\n    // 添加更多用户\n    ];\n  },\n  methods: {\n    addUser() {\n      // 添加新用户\n      this.users.push({\n        username: this.newUser.username,\n        userType: this.newUser.userType\n      });\n      // 清空表单数据\n      this.newUser.username = '';\n      this.newUser.userType = '';\n    },\n    editUser(user) {\n      // 编辑用户，可以弹出模态框进行编辑\n      console.log('编辑用户:', user);\n    },\n    deleteUser(user) {\n      // 删除用户，可以弹出确认对话框\n      const index = this.users.indexOf(user);\n      if (index !== -1) {\n        this.users.splice(index, 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","users","newUser","username","userType","mounted","methods","addUser","push","editUser","user","console","log","deleteUser","index","indexOf","splice"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/UserManagement.vue"],"sourcesContent":["<template>\n  <div class=\"user-management\">\n    <h2>用户管理</h2>\n    <form @submit.prevent=\"addUser\">\n      <input type=\"text\" v-model=\"newUser.username\" placeholder=\"用户名\">\n      <input type=\"text\" v-model=\"newUser.userType\" placeholder=\"用户类型\">\n      <button type=\"submit\">添加用户</button>\n    </form>\n    \n    <table>\n      <thead>\n        <tr>\n          <th>用户名</th>\n          <th>用户类型</th>\n          <th>操作</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(user, index) in users\" :key=\"index\">\n          <td>{{ user.username }}</td>\n          <td>{{ user.userType }}</td>\n          <td>\n            <button @click=\"editUser(user)\">编辑</button>\n            <button @click=\"deleteUser(user)\">删除</button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      users: [], // 用户列表\n      newUser: { username: '', userType: '' } // 添加新用户表单数据\n    };\n  },\n  mounted() {\n    // 模拟从后端获取用户列表\n    this.users = [\n      { username: '用户1', userType: '管理员' },\n      // 添加更多用户\n    ];\n  },\n  methods: {\n    addUser() {\n      // 添加新用户\n      this.users.push({ username: this.newUser.username, userType: this.newUser.userType });\n      // 清空表单数据\n      this.newUser.username = '';\n      this.newUser.userType = '';\n    },\n    editUser(user) {\n      // 编辑用户，可以弹出模态框进行编辑\n      console.log('编辑用户:', user);\n    },\n    deleteUser(user) {\n      // 删除用户，可以弹出确认对话框\n      const index = this.users.indexOf(user);\n      if (index !== -1) {\n        this.users.splice(index, 1);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* 样式 */\n</style>\n"],"mappings":";AAgCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,OAAO,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,EAAE;IAC1C,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACJ,KAAI,GAAI,CACX;MAAEE,QAAQ,EAAE,KAAK;MAAEC,QAAQ,EAAE;IAAM;IACnC;IAAA,CACD;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR;MACA,IAAI,CAACN,KAAK,CAACO,IAAI,CAAC;QAAEL,QAAQ,EAAE,IAAI,CAACD,OAAO,CAACC,QAAQ;QAAEC,QAAQ,EAAE,IAAI,CAACF,OAAO,CAACE;MAAS,CAAC,CAAC;MACrF;MACA,IAAI,CAACF,OAAO,CAACC,QAAO,GAAI,EAAE;MAC1B,IAAI,CAACD,OAAO,CAACE,QAAO,GAAI,EAAE;IAC5B,CAAC;IACDK,QAAQA,CAACC,IAAI,EAAE;MACb;MACAC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,IAAI,CAAC;IAC5B,CAAC;IACDG,UAAUA,CAACH,IAAI,EAAE;MACf;MACA,MAAMI,KAAI,GAAI,IAAI,CAACb,KAAK,CAACc,OAAO,CAACL,IAAI,CAAC;MACtC,IAAII,KAAI,KAAM,CAAC,CAAC,EAAE;QAChB,IAAI,CAACb,KAAK,CAACe,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;MAC7B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}