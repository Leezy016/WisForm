{"ast":null,"code":"import DynamicForm from './DynamicForm.vue';\nimport NavBar from './NavBar.vue';\nexport default {\n  components: {\n    NavBar,\n    DynamicForm\n  }\n};","map":{"version":3,"names":["DynamicForm","NavBar","components"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormCreate.vue"],"sourcesContent":["\r\n<template>\r\n    <div class=\"form-create\"> \r\n        <NavBar/> <!-- 引入并使用NavBar组件 -->  \r\n        \r\n        <div class=\"form-title-input\">  \r\n          <label for=\"formTitle\">表单标题：</label>  \r\n          <input type=\"text\" id=\"formTitle\" v-model=\"formTitle\" placeholder=\"请输入表单标题\">  \r\n        </div> \r\n\r\n        <div v-for=\"(field, index) in selectedFields\" :key=\"index\" class=\"form-item\">\r\n          <input type=\"text\" v-model=\"field.label\" placeholder=\"表单项名称\" class=\"field-input\">\r\n            <select v-model=\"field.type\" class=\"field-select\">\r\n              <option value=\"text\">文本</option>\r\n              <option value=\"number\">数字</option>\r\n              <!-- 可以根据需要添加更多的选项 -->\r\n            </select>\r\n          <button @click=\"removeField(index)\" class=\"remove-btn\">-</button>\r\n        </div>\r\n\r\n        <button @click=\"addField\" class=\"add-btn\">添加表单项</button>\r\n        <button @click=\"generateForm\" class=\"generate-btn\">确认</button>\r\n\r\n        <form v-if=\"showForm\" @submit.prevent=\"submitForm\" class=\"form-table\">  \r\n          <table>  \r\n            <tr v-for=\"(field, index) in selectedFields\" :key=\"index\">  \r\n              <td>  \r\n                <label :for=\"field.id\" class=\"field-label\">{{ field.label }}</label>  \r\n              </td>  \r\n              <td>  \r\n            <input :type=\"field.type\" :id=\"field.id\" v-model=\"formData[field.id]\" class=\"field-input\">  \r\n          </td>  \r\n        </tr>  \r\n      </table>  \r\n      <button type=\"submit\" class=\"submit-btn\">提交</button>  \r\n    </form>\r\n\r\n    </div>\r\n  </template>\r\n\r\n  <script>\r\n\r\n  import DynamicForm from './DynamicForm.vue';\r\n  import NavBar from './NavBar.vue'; \r\n  export default {\r\n    components: {  \r\n    NavBar,\r\n    DynamicForm\r\n  }  \r\n  };\r\n  </script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA0CE,OAAOA,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,MAAK,MAAO,cAAc;AACjC,eAAe;EACbC,UAAU,EAAE;IACZD,MAAM;IACND;EACF;AACA,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}