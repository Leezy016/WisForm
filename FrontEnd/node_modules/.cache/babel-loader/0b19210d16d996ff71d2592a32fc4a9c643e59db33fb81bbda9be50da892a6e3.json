{"ast":null,"code":"import NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  props: ['title'],\n  // 接收从父组件传递过来的 title \n  data() {\n    return {\n      item: ['作者'],\n      content: [],\n      getErrorMessage: '',\n      submitErrorMessage: ''\n    };\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    getItems() {\n      axios.post('http://localhost:8080/api/register', {\n        title: this.title\n      }).then(response => {\n        if (response.data.success) {\n          //console.log('表单内容获取成功'); \n          this.item = response.data.item;\n        } else {\n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;\n        } else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });\n    }\n  },\n  submitForm() {\n    console.log(this.content);\n    axios.post('http://localhost:8080/api/register', {\n      item: this.item\n    }).then(response => {\n      if (response.data.success) {\n        //console.log('表单内容获取成功'); \n        this.item = response.data.item;\n      } else {\n        this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';\n      }\n    }).catch(error => {\n      if (error.response) {\n        // 后端返回的错误信息\n        this.getErrorMessage = error.response.data;\n      } else {\n        this.getErrorMessage = '表单内容获取失败，请稍后再试';\n      }\n    });\n  },\n  created() {\n    //this.getItems(); \n    this.content = Array(this.item.length).fill('');\n  }\n};","map":{"version":3,"names":["NavBar","axios","props","data","item","content","getErrorMessage","submitErrorMessage","components","methods","getItems","post","title","then","response","success","message","catch","error","submitForm","console","log","created","Array","length","fill"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormFillDetail.vue"],"sourcesContent":["<template>  \n    <div>\n      <NavBar/>\n    </div>\n    <div class=\"form-detail\">  \n      <p>{{ title }}</p>  \n      <!-- 在这里添加其他详情页面的内容 -->  \n      <div v-for=\"(itemName, index) in item\" :key=\"index\" class=\"form-group\">  \n        <label>{{ itemName }}</label>  \n          <input v-model=\"content[index]\" />  \n        </div>  \n        <button type=\"button\" class=\"submit-btn\" @click=\"submitForm\">提交</button> \n    </div>  \n  </template>  \n    \n  <script>  \n  import NavBar from './NavBar.vue'; \n  import axios from 'axios';\n  export default {  \n    props: ['title'], // 接收从父组件传递过来的 title \n    data() {  \n    return {  \n      item: ['作者'],\n      content:[],\n      getErrorMessage:'',\n      submitErrorMessage:''\n    };  \n    },  \n    components: {\n      NavBar,\n    },\n    methods:{\n    getItems() {   \n      axios.post('http://localhost:8080/api/register', {\n        title:this.title\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单内容获取成功'); \n          this.item=response.data.item\n        }\n        else {  \n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';  \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;   \n        }\n        else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });  \n    },   \n    }, \n    submitForm() {  \n      console.log(this.content);  \n      axios.post('http://localhost:8080/api/register', {\n        item:this.item\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单内容获取成功'); \n          this.item=response.data.item\n        }\n        else {  \n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';  \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;   \n        }\n        else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });  \n    },   \n    created() {  \n      //this.getItems(); \n      this.content = Array(this.item.length).fill(''); \n    },\n\n  };  \n  </script>\n\n  <style>\n  .form-detail{\n    margin: auto;\n  }\n</style>"],"mappings":"AAgBE,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAK,EAAE,CAAC,OAAO,CAAC;EAAE;EAClBC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAE,CAAC,IAAI,CAAC;MACZC,OAAO,EAAC,EAAE;MACVC,eAAe,EAAC,EAAE;MAClBC,kBAAkB,EAAC;IACrB,CAAC;EACD,CAAC;EACDC,UAAU,EAAE;IACVR;EACF,CAAC;EACDS,OAAO,EAAC;IACRC,QAAQA,CAAA,EAAG;MACTT,KAAK,CAACU,IAAI,CAAC,oCAAoC,EAAE;QAC/CC,KAAK,EAAC,IAAI,CAACA;MACb,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACX,IAAI,CAACY,OAAO,EAAE;UACzB;UACA,IAAI,CAACX,IAAI,GAACU,QAAQ,CAACX,IAAI,CAACC,IAAG;QAC7B,OACK;UACH,IAAI,CAACE,eAAc,GAAIQ,QAAQ,CAACX,IAAI,CAACa,OAAM,IAAK,gBAAgB;QAClE;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACJ,QAAQ,EAAE;UAClB;UACA,IAAI,CAACR,eAAc,GAAIY,KAAK,CAACJ,QAAQ,CAACX,IAAI;QAC5C,OACK;UACH,IAAI,CAACG,eAAc,GAAI,gBAAgB;QACzC;MACF,CAAC,CAAC;IACJ;EACA,CAAC;EACDa,UAAUA,CAAA,EAAG;IACXC,OAAO,CAACC,GAAG,CAAC,IAAI,CAAChB,OAAO,CAAC;IACzBJ,KAAK,CAACU,IAAI,CAAC,oCAAoC,EAAE;MAC/CP,IAAI,EAAC,IAAI,CAACA;IACZ,CAAC,EACAS,IAAI,CAACC,QAAO,IAAK;MAChB,IAAIA,QAAQ,CAACX,IAAI,CAACY,OAAO,EAAE;QACzB;QACA,IAAI,CAACX,IAAI,GAACU,QAAQ,CAACX,IAAI,CAACC,IAAG;MAC7B,OACK;QACH,IAAI,CAACE,eAAc,GAAIQ,QAAQ,CAACX,IAAI,CAACa,OAAM,IAAK,gBAAgB;MAClE;IACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACd,IAAIA,KAAK,CAACJ,QAAQ,EAAE;QAClB;QACA,IAAI,CAACR,eAAc,GAAIY,KAAK,CAACJ,QAAQ,CAACX,IAAI;MAC5C,OACK;QACH,IAAI,CAACG,eAAc,GAAI,gBAAgB;MACzC;IACF,CAAC,CAAC;EACJ,CAAC;EACDgB,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACjB,OAAM,GAAIkB,KAAK,CAAC,IAAI,CAACnB,IAAI,CAACoB,MAAM,CAAC,CAACC,IAAI,CAAC,EAAE,CAAC;EACjD;AAEF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDep