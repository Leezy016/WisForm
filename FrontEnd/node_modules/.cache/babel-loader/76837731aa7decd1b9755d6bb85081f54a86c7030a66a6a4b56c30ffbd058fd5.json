{"ast":null,"code":"export default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      identity: 'user',\n      // 默认选择普通用户\n      loginError: '' // 登录错误信息\n    };\n  },\n  methods: {\n    login() {\n      // 模拟登录验证，实际应替换为后端接口调用\n      if (this.username === 'admin' && this.password === 'admin' && this.identity === 'admin') {\n        // 管理员身份登录成功\n        this.loginError = '';\n        alert('管理员登录成功！');\n      } else if (this.username === 'user' && this.password === 'user' && this.identity === 'user') {\n        // 普通用户身份登录成功\n        this.loginError = '';\n        alert('普通用户登录成功！');\n      } else {\n        // 登录失败，显示错误信息\n        this.loginError = '用户名或密码错误，请重新输入！';\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","username","password","identity","loginError","methods","login","alert"],"sources":["C:\\Users\\32118\\Desktop\\无脑赶工\\my-project\\src\\components\\HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"login\">\n    <h2>用户登录</h2>\n    <form @submit.prevent=\"login\">\n      <div>\n        <label for=\"username\">用户名：</label>\n        <input type=\"text\" id=\"username\" v-model=\"username\" placeholder=\"请输入用户名\" required>\n      </div>\n      <div>\n        <label for=\"password\">密码：</label>\n        <input type=\"password\" id=\"password\" v-model=\"password\" placeholder=\"请输入密码\" required>\n      </div>\n      <div>\n        <label>身份：</label>\n        <label><input type=\"radio\" v-model=\"identity\" value=\"admin\">管理员</label>\n        <label><input type=\"radio\" v-model=\"identity\" value=\"user\">普通用户</label>\n      </div>\n      <button type=\"submit\">登录</button>\n    </form>\n    <p v-if=\"loginError\" class=\"error-message\">{{ loginError }}</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      identity: 'user', // 默认选择普通用户\n      loginError: '' // 登录错误信息\n    };\n  },\n  methods: {\n    login() {\n      // 模拟登录验证，实际应替换为后端接口调用\n      if (this.username === 'admin' && this.password === 'admin' && this.identity === 'admin') {\n        // 管理员身份登录成功\n        this.loginError = '';\n        alert('管理员登录成功！');\n      } else if (this.username === 'user' && this.password === 'user' && this.identity === 'user') {\n        // 普通用户身份登录成功\n        this.loginError = '';\n        alert('普通用户登录成功！');\n      } else {\n        // 登录失败，显示错误信息\n        this.loginError = '用户名或密码错误，请重新输入！';\n      }\n    }\n  }\n};\n</script>\n\n"],"mappings":"AAwBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,MAAM;MAAE;MAClBC,UAAU,EAAE,EAAC,CAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN;MACA,IAAI,IAAI,CAACL,QAAO,KAAM,OAAM,IAAK,IAAI,CAACC,QAAO,KAAM,OAAM,IAAK,IAAI,CAACC,QAAO,KAAM,OAAO,EAAE;QACvF;QACA,IAAI,CAACC,UAAS,GAAI,EAAE;QACpBG,KAAK,CAAC,UAAU,CAAC;MACnB,OAAO,IAAI,IAAI,CAACN,QAAO,KAAM,MAAK,IAAK,IAAI,CAACC,QAAO,KAAM,MAAK,IAAK,IAAI,CAACC,QAAO,KAAM,MAAM,EAAE;QAC3F;QACA,IAAI,CAACC,UAAS,GAAI,EAAE;QACpBG,KAAK,CAAC,WAAW,CAAC;MACpB,OAAO;QACL;QACA,IAAI,CAACH,UAAS,GAAI,iBAAiB;MACrC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}