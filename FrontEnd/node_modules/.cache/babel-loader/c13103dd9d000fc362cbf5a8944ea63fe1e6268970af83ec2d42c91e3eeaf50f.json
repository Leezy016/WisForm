{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  props: ['title'],\n  // 接收从父组件传递过来的 title \n  data() {\n    return {\n      item: [],\n      content: [],\n      getErrorMessage: '',\n      submitErrorMessage: '',\n      matchErrorMessage: '',\n      keys: [\"姓名\", \"作者姓名\", \"项目名称\", \"论文标题\"],\n      itemValues: [\"院长\"],\n      //匹配上的\n      selectedValue: '',\n      isKey: false\n    };\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    onValueChange() {\n      // 当选中值改变时触发的方法（可选）  \n      console.log('Selected value changed to:', this.selectedValue);\n    },\n    Match(item, itemValue) {\n      console.log(`Key match for item: ${item}, value: ${itemValue}`);\n      axios.post('http://localhost:8080/key-match', {\n        item: item,\n        itemValue: itemValue\n      }).then(response => {\n        if (response.data.success) {\n          this.itemValues = response.data.itemValues;\n        }\n      });\n    },\n    keyMatch(item, itemValue) {\n      console.log(`Match for item: ${item}, value: ${itemValue}`);\n      // ... 你的逻辑  \n    },\n    keyJudge(item, itemValue) {\n      for (let i = 0; i < this.keys.length; i++) {\n        if (this.keys[i] === item) {\n          this.isKey = true;\n          break;\n        }\n      }\n      if (this.isKey === false) {\n        this.Match(item, itemValue);\n      }\n    },\n    getItems() {\n      axios.post('http://localhost:8080/fillform-desplay', {\n        name: this.title\n      }).then(response => {\n        if (response.data.success) {\n          this.item = response.data.item;\n        } else {\n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;\n        } else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });\n    },\n    submitForm() {\n      console.log(this.content);\n      axios.post('http://localhost:8080/fillform-recieve', {\n        title: this.title,\n        filler: this.$store.state.username,\n        Item: this.item,\n        ItemValue: this.content\n      }).then(response => {\n        if (response.data.success) {\n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push('/form-fill');\n        } else {\n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试';\n          alert(this.submiterrorMessage);\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;\n        } else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });\n    },\n    goBack() {\n      this.$router.push('/form-fill');\n    }\n  },\n  created() {\n    this.getItems();\n    this.content = Array(this.item.length).fill('');\n  }\n};","map":{"version":3,"names":["NavBar","axios","props","data","item","content","getErrorMessage","submitErrorMessage","matchErrorMessage","keys","itemValues","selectedValue","isKey","components","methods","onValueChange","console","log","Match","itemValue","post","then","response","success","keyMatch","keyJudge","i","length","getItems","name","title","message","catch","error","submitForm","filler","$store","state","username","Item","ItemValue","alert","$router","push","submiterrorMessage","goBack","created","Array","fill"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormFillDetail.vue"],"sourcesContent":["<template>  \n    <div>\n      <NavBar/>\n    </div>\n    <div class=\"form-detail\">  \n      <p>{{ title }}</p>  \n\n      <div v-for=\"(itemName, index) in item\" :key=\"index\" class=\"form-group\">  \n        <label>{{ itemName }}</label>  \n          <input v-model=\"content[index]\" @click=\"keyJudge(item[index],content[index])\" />\n          <li  @change=\"onValueChange\">  \n            <option v-for=\"item in itemValues\" :key=\"item\" :value=\"item\">  \n              {{ item }}  \n            </option>  \n          </li>   \n        </div>  \n\n        <div id = \"app\" v-cloak>\n \n <li v-for=\"(itemValue,index) in itemValues\" :key=\"index\" @mouseover=\"menu.show = !menu.show\" @mouseout=\"menu.show = !menu.show\">\n     <p >\n         {{itemValue}}\n     </p>\n\n     <ul>\n\n         <li v-for=\"(itemValue,index) in itemValues\" :key=\"index\">\n          <p >\n         {{itemValue}}\n     </p>\n\n         </li>\n\n     </ul>\n\n </li>\n\n</div>\n\n        <button type=\"button\" class=\"submit-btn\" @click=\"submitForm\">提交</button> \n        <button type=\"button\" class=\"return-btn\" @click=\"goBack\">返回</button>\n        <p v-if=\"getErrorMessage\" class=\"errorMessage\">{{ getErrorMessage }}</p> \n        <p v-if=\"submitErrorMessage\" class=\"errorMessage\">{{ submitErrorMessage }}</p> \n    </div>  \n  </template>  \n    \n  <script>  \n  import NavBar from './NavBar.vue'; \n  import axios from 'axios';\n  export default {  \n    props: ['title'], // 接收从父组件传递过来的 title \n    data() {  \n    return {  \n      item: [],\n      content:[],\n      getErrorMessage:'',\n      submitErrorMessage:'',\n      matchErrorMessage:'',\n      keys:[\"姓名\",\"作者姓名\",\"项目名称\",\"论文标题\"],\n      itemValues:[\"院长\"],//匹配上的\n      selectedValue: '',\n      isKey:false,\n    };  \n    },  \n    components: {\n      NavBar,\n    },\n    methods:{\n      onValueChange() {  \n      // 当选中值改变时触发的方法（可选）  \n      console.log('Selected value changed to:', this.selectedValue);  \n    },  \n    Match(item, itemValue) {  \n      console.log(`Key match for item: ${item}, value: ${itemValue}`); \n      axios.post('http://localhost:8080/key-match', {  \n        item:item,\n        itemValue:itemValue\n        })  \n        .then(response => {  \n          if (response.data.success) {  \n            this.itemValues=response.data.itemValues\n          }\n         })\n    }, \n    keyMatch(item, itemValue) {  \n      console.log(`Match for item: ${item}, value: ${itemValue}`);  \n      // ... 你的逻辑  \n    },\n    keyJudge(item,itemValue){  \n      for (let i = 0; i < this.keys.length; i++) {   \n        if (this.keys[i] === item) {  \n          this.isKey = true;  \n          break;  \n        }  \n      }  \n      if (this.isKey===false) {  \n        this.Match(item, itemValue);  \n      }  \n    },\n    getItems() {   \n      axios.post('http://localhost:8080/fillform-desplay', {\n        name:this.title,\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          this.item=response.data.item;\n        }\n        else {  \n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';  \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;   \n        }\n        else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });  \n    },  \n    submitForm() {  \n      console.log(this.content);  \n      axios.post('http://localhost:8080/fillform-recieve', {\n        title:this.title,\n        filler:this.$store.state.username,\n        Item:this.item,\n        ItemValue:this.content\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push('/form-fill');\n        }\n        else {  \n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试'; \n          alert(this.submiterrorMessage); \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;   \n        }\n        else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });  \n    },   \n    goBack(){\n      this.$router.push('/form-fill');\n    }, \n    }, \n    \n    created() {  \n      this.getItems(); \n      this.content = Array(this.item.length).fill(''); \n    },\n\n  };  \n  </script>\n\n  <style>\n  .form-detail{\n    margin: auto;\n  }\n</style>\n"],"mappings":";AA+CE,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAK,EAAE,CAAC,OAAO,CAAC;EAAE;EAClBC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAC,EAAE;MACVC,eAAe,EAAC,EAAE;MAClBC,kBAAkB,EAAC,EAAE;MACrBC,iBAAiB,EAAC,EAAE;MACpBC,IAAI,EAAC,CAAC,IAAI,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,CAAC;MAChCC,UAAU,EAAC,CAAC,IAAI,CAAC;MAAC;MAClBC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAC;IACR,CAAC;EACD,CAAC;EACDC,UAAU,EAAE;IACVb;EACF,CAAC;EACDc,OAAO,EAAC;IACNC,aAAaA,CAAA,EAAG;MAChB;MACAC,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAE,IAAI,CAACN,aAAa,CAAC;IAC/D,CAAC;IACDO,KAAKA,CAACd,IAAI,EAAEe,SAAS,EAAE;MACrBH,OAAO,CAACC,GAAG,CAAE,uBAAsBb,IAAK,YAAWe,SAAU,EAAC,CAAC;MAC/DlB,KAAK,CAACmB,IAAI,CAAC,iCAAiC,EAAE;QAC5ChB,IAAI,EAACA,IAAI;QACTe,SAAS,EAACA;MACV,CAAC,EACAE,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACnB,IAAI,CAACoB,OAAO,EAAE;UACzB,IAAI,CAACb,UAAU,GAACY,QAAQ,CAACnB,IAAI,CAACO,UAAS;QACzC;MACD,CAAC;IACN,CAAC;IACDc,QAAQA,CAACpB,IAAI,EAAEe,SAAS,EAAE;MACxBH,OAAO,CAACC,GAAG,CAAE,mBAAkBb,IAAK,YAAWe,SAAU,EAAC,CAAC;MAC3D;IACF,CAAC;IACDM,QAAQA,CAACrB,IAAI,EAACe,SAAS,EAAC;MACtB,KAAK,IAAIO,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACjB,IAAI,CAACkB,MAAM,EAAED,CAAC,EAAE,EAAE;QACzC,IAAI,IAAI,CAACjB,IAAI,CAACiB,CAAC,MAAMtB,IAAI,EAAE;UACzB,IAAI,CAACQ,KAAI,GAAI,IAAI;UACjB;QACF;MACF;MACA,IAAI,IAAI,CAACA,KAAK,KAAG,KAAK,EAAE;QACtB,IAAI,CAACM,KAAK,CAACd,IAAI,EAAEe,SAAS,CAAC;MAC7B;IACF,CAAC;IACDS,QAAQA,CAAA,EAAG;MACT3B,KAAK,CAACmB,IAAI,CAAC,wCAAwC,EAAE;QACnDS,IAAI,EAAC,IAAI,CAACC;MACZ,CAAC,EACAT,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACnB,IAAI,CAACoB,OAAO,EAAE;UACzB,IAAI,CAACnB,IAAI,GAACkB,QAAQ,CAACnB,IAAI,CAACC,IAAI;QAC9B,OACK;UACH,IAAI,CAACE,eAAc,GAAIgB,QAAQ,CAACnB,IAAI,CAAC4B,OAAM,IAAK,gBAAgB;QAClE;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACX,QAAQ,