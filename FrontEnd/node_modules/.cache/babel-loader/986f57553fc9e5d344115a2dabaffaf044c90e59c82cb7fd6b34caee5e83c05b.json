{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from './NavBar.vue';\nimport axios from 'axios';\nexport default {\n  props: ['title'],\n  // 接收从父组件传递过来的 title \n  data() {\n    return {\n      item: [],\n      content: [],\n      getErrorMessage: '',\n      submitErrorMessage: '',\n      keys: [\"姓名\", \"作者姓名\", \"项目名称\", \"论文标题\"],\n      isKey: false\n    };\n  },\n  components: {\n    NavBar\n  },\n  methods: {\n    Match(item, itemValue) {\n      console.log(`Key match for item: ${item}, value: ${itemValue}`);\n    },\n    keyMatch(item, itemValue) {\n      console.log(`Match for item: ${item}, value: ${itemValue}`);\n      // ... 你的逻辑  \n    },\n    dataMatch(item, itemValue) {\n      for (let i = 0; i < this.keys.length; i++) {\n        if (this.keys[i] === item) {\n          this.isKey = true;\n          break;\n        }\n      }\n      if (this.isKey === false) {\n        this.Match(item, itemValue);\n      }\n    },\n    getItems() {\n      axios.post('http://localhost:8080/fillform-desplay', {\n        name: this.title\n      }).then(response => {\n        if (response.data.success) {\n          this.item = response.data.item;\n        } else {\n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;\n        } else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });\n    },\n    submitForm() {\n      console.log(this.content);\n      axios.post('http://localhost:8080/fillform-recieve', {\n        title: this.title,\n        filler: this.$store.state.username,\n        Item: this.item,\n        ItemValue: this.content\n      }).then(response => {\n        if (response.data.success) {\n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push('/form-fill');\n        } else {\n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试';\n          alert(this.submiterrorMessage);\n        }\n      }).catch(error => {\n        if (error.response) {\n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;\n        } else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });\n    },\n    goBack() {\n      this.$router.push('/form-fill');\n    }\n  },\n  created() {\n    this.getItems();\n    this.content = Array(this.item.length).fill('');\n  }\n};","map":{"version":3,"names":["NavBar","axios","props","data","item","content","getErrorMessage","submitErrorMessage","keys","isKey","components","methods","Match","itemValue","console","log","keyMatch","dataMatch","i","length","getItems","post","name","title","then","response","success","message","catch","error","submitForm","filler","$store","state","username","Item","ItemValue","alert","$router","push","submiterrorMessage","goBack","created","Array","fill"],"sources":["/Users/aoliliaoao/Downloads/WisForm/FrontEnd/src/components/FormFillDetail.vue"],"sourcesContent":["<template>  \n    <div>\n      <NavBar/>\n    </div>\n    <div class=\"form-detail\">  \n      <p>{{ title }}</p>  \n      <!-- 在这里添加其他详情页面的内容 -->  \n      <div v-for=\"(itemName, index) in item\" :key=\"index\" class=\"form-group\">  \n        <label>{{ itemName }}</label>  \n          <input v-model=\"content[index]\" @click=\"dataMatch(item[index],content[index])\" />  \n        </div>  \n        <button type=\"button\" class=\"submit-btn\" @click=\"submitForm\">提交</button> \n        <button type=\"button\" class=\"return-btn\" @click=\"goBack\">返回</button>\n        <p v-if=\"getErrorMessage\" class=\"errorMessage\">{{ getErrorMessage }}</p> \n        <p v-if=\"submitErrorMessage\" class=\"errorMessage\">{{ submitErrorMessage }}</p> \n    </div>  \n  </template>  \n    \n  <script>  \n  import NavBar from './NavBar.vue'; \n  import axios from 'axios';\n  export default {  \n    props: ['title'], // 接收从父组件传递过来的 title \n    data() {  \n    return {  \n      item: [],\n      content:[],\n      getErrorMessage:'',\n      submitErrorMessage:'',\n      keys:[\"姓名\",\"作者姓名\",\"项目名称\",\"论文标题\"],\n      isKey:false,\n    };  \n    },  \n    components: {\n      NavBar,\n    },\n    methods:{\n    Match(item, itemValue) {  \n      console.log(`Key match for item: ${item}, value: ${itemValue}`);  \n    }, \n    keyMatch(item, itemValue) {  \n      console.log(`Match for item: ${item}, value: ${itemValue}`);  \n      // ... 你的逻辑  \n    },\n    dataMatch(item,itemValue){  \n      for (let i = 0; i < this.keys.length; i++) {   \n        if (this.keys[i] === item) {  \n          this.isKey = true;  \n          break;  \n        }  \n      }  \n      if (this.isKey===false) {  \n        this.Match(item, itemValue);  \n      }  \n    },\n    getItems() {   \n      axios.post('http://localhost:8080/fillform-desplay', {\n        name:this.title,\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          this.item=response.data.item;\n        }\n        else {  \n          this.getErrorMessage = response.data.message || '表单内容获取失败，请稍后再试';  \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.getErrorMessage = error.response.data;   \n        }\n        else {\n          this.getErrorMessage = '表单内容获取失败，请稍后再试';\n        }\n      });  \n    },  \n    submitForm() {  \n      console.log(this.content);  \n      axios.post('http://localhost:8080/fillform-recieve', {\n        title:this.title,\n        filler:this.$store.state.username,\n        Item:this.item,\n        ItemValue:this.content\n      })  \n      .then(response => {  \n        if (response.data.success) { \n          //console.log('表单提交成功'); \n          alert('表单提交成功！');\n          this.$router.push('/form-fill');\n        }\n        else {  \n          this.submitErrorMessage = response.data.message || '表单提交失败，请稍后再试'; \n          alert(this.submiterrorMessage); \n        }\n      })  \n      .catch(error => {  \n        if (error.response) {  \n          // 后端返回的错误信息\n          this.submitErrorMessage = error.response.data;   \n        }\n        else {\n          this.submitErrorMessage = '表单提交失败，请稍后再试';\n        }\n      });  \n    },   \n    goBack(){\n      this.$router.push('/form-fill');\n    }, \n    }, \n    \n    created() {  \n      this.getItems(); \n      this.content = Array(this.item.length).fill(''); \n    },\n\n  };  \n  </script>\n\n  <style>\n  .form-detail{\n    margin: auto;\n  }\n</style>\n"],"mappings":";AAmBE,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAK,EAAE,CAAC,OAAO,CAAC;EAAE;EAClBC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAC,EAAE;MACVC,eAAe,EAAC,EAAE;MAClBC,kBAAkB,EAAC,EAAE;MACrBC,IAAI,EAAC,CAAC,IAAI,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,CAAC;MAChCC,KAAK,EAAC;IACR,CAAC;EACD,CAAC;EACDC,UAAU,EAAE;IACVV;EACF,CAAC;EACDW,OAAO,EAAC;IACRC,KAAKA,CAACR,IAAI,EAAES,SAAS,EAAE;MACrBC,OAAO,CAACC,GAAG,CAAE,uBAAsBX,IAAK,YAAWS,SAAU,EAAC,CAAC;IACjE,CAAC;IACDG,QAAQA,CAACZ,IAAI,EAAES,SAAS,EAAE;MACxBC,OAAO,CAACC,GAAG,CAAE,mBAAkBX,IAAK,YAAWS,SAAU,EAAC,CAAC;MAC3D;IACF,CAAC;IACDI,SAASA,CAACb,IAAI,EAACS,SAAS,EAAC;MACvB,KAAK,IAAIK,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACV,IAAI,CAACW,MAAM,EAAED,CAAC,EAAE,EAAE;QACzC,IAAI,IAAI,CAACV,IAAI,CAACU,CAAC,MAAMd,IAAI,EAAE;UACzB,IAAI,CAACK,KAAI,GAAI,IAAI;UACjB;QACF;MACF;MACA,IAAI,IAAI,CAACA,KAAK,KAAG,KAAK,EAAE;QACtB,IAAI,CAACG,KAAK,CAACR,IAAI,EAAES,SAAS,CAAC;MAC7B;IACF,CAAC;IACDO,QAAQA,CAAA,EAAG;MACTnB,KAAK,CAACoB,IAAI,CAAC,wCAAwC,EAAE;QACnDC,IAAI,EAAC,IAAI,CAACC;MACZ,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACtB,IAAI,CAACuB,OAAO,EAAE;UACzB,IAAI,CAACtB,IAAI,GAACqB,QAAQ,CAACtB,IAAI,CAACC,IAAI;QAC9B,OACK;UACH,IAAI,CAACE,eAAc,GAAImB,QAAQ,CAACtB,IAAI,CAACwB,OAAM,IAAK,gBAAgB;QAClE;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACJ,QAAQ,EAAE;UAClB;UACA,IAAI,CAACnB,eAAc,GAAIuB,KAAK,CAACJ,